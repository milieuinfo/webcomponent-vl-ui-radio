{
  "name": "vl-ui-radio",
  "version": "0.0.0",
  "description": "A radio button allows a user to select a single option from a list. Avoid preselecting a radio button because it is important for the user to make a conscious selection.",
  "main": "vl-radio.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/milieuinfo/webcomponent-vl-ui-radio.git"
  },
  "author": "DIDM",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/milieuinfo/webcomponent-vl-ui-radio/issues"
  },
  "homepage": "https://github.com/milieuinfo/webcomponent-vl-ui-radio#readme",
  "scripts": {
    "postinstall": "npm explore vl-ui-util -- npm run util",
    "test": "wct -l chrome,firefox --npm",
    "release": "np",
    "release:prepare": "npm run release:prepare:build && npm run release:prepare:imports && npm run release:prepare:css && npm run release:prepare:commit",
    "release:prepare:build": "uglifyjs --compress --mangle --output vl-radio.js -- *.src.js",
    "release:prepare:imports": "replace '/node_modules/vl-ui-core/vl-core.src.js' '/node_modules/vl-ui-core/vl-core.js' vl-radio.js",
    "release:prepare:css": "replace '\\.\\./style.css' '/node_modules/vl-ui-radio/style.css' vl-radio.js",
    "release:prepare:commit": "git add -f vl-radio.js && git commit --amend --no-edit && git pull",
    "release:testless": "np --yolo",
    "demo": "http-server"
  },
  "dependencies": {
    "@govflanders/vl-ui-core": "^3.7.2",
    "@govflanders/vl-ui-form-message": "3.9.0",
    "@govflanders/vl-ui-form-structure": "3.9.0",
    "@govflanders/vl-ui-util": "3.9.0",
    "vl-ui-core": "2.1.0"
  },
  "peerDependencies": {
    "vl-ui-core": "2.1.0"
  },
  "devDependencies": {
    "@govflanders/vl-ui-radio": "^3.7.2",
    "http-server": "0.11.1",
    "np": "3.1.0",
    "replace": "1.1.1",
    "uglify-js-es6": "2.8.9",
    "vl-ui-icon": "2.1.1",
    "vl-ui-util": "^2.0.0",
    "wct-browser-legacy": "1.0.2",
    "web-component-tester": "6.9.2"
  },
  "publishConfig": {
    "registry": "http://artifactory-pr-build.lb.cumuli.be:8081/artifactory/api/npm/acd-npm/"
  }
}
